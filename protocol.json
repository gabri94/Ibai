LOGIN
  request:
    {
      "msg_id": 0,
      "user": user,
      "pass": md5 of pwd
    }

  response:
    {
      "msg_id": -1,
      "response": 0,
      "code": [session_id] or 0
    }

LOGOUT
  request:
    {
      "session_id": [id],
      "msg_id": 0
    }
  response:
    {
      "msg_id": -1,
      "response": 0,
      "code": 1 or 0
    }

REGISTER NOTIFICATION ENDPOINT
  request:
    {
      "session_id": [id],
      "msg_id": 4,
      "host": host,
      "port": port,
    }
  response
    {
      "msg_id": -1,
      "response": 4,
      "code": 1 or 0
    }


REGISTER CATEGORY
  request
    {
      "session_id": [id],
      "msg_id": 1,
      "category": cat_name
    }
  response
    {
      "msg_id": -1,
      "response": 1,
      "code": 1 or 0
    }

LIST CATEGORIES
  request
    {
      "session_id": [id],
      "msg_id": 10,
    }
  response
  {
    "msg_id": -1,
    "response": 10,
    "cat_list": []
  }


SEARCH CATEGORIES
  request
    {
      "session_id": [id],
      "msg_id": 11,
      "category": ""
    }
  response
    {
      "msg_id": -1,
      "response": 11,
      "cat_list": []
    }



ADD PRODUCT
  request
    {
      "session_id": [id],
      "msg_id": 2,
      "category": cat_name,
      "product": prod_name,
      "price": price
    }
  response
    {
      "msg_id": -1,
      "response": 2,
      "code": 1 or 0
    }
LIST PRODUCT
  request
    {
      "session_id": [id],
      "msg_id": 20
      "category": [cat_name]
    }
  response
    {
      "msg_id": -1,
      "response": 20,
      "prod_list": []
    }
SEARCH PRODUCTS
  request
    {
      "session_id": [id],
      "msg_id": 21
      "category": [cat_name]
      "product": [search_terms]
    }
  response
    {
      "msg_id": -1,
      "response": 21,
      "prod_list": []
    }
BID PRODUCT
  request:
    {
      "session_id": [id],
      "msg_id": 3,
      "category": cat_name,
      "product": prod_name,
      "price": price
    }
  response
    {
        "msg_id": -1,
        "response": 3,
        "code": 1 or 0
    }

generic RESPONSE
{
  "msg_id": -1,
  "response": [msg_id],
  "code": res_code
}

NOTIFICATION
{
  "msg_id": 40,
  "notif_code": [code],
  "notif_msg": [text],
}
